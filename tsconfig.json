{
  "compilerOptions": {
    "outDir": "./dist",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es6",
    "jsx": "react",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "renderer/*"
      ]
    }
  },
  "include": [
    "./renderer/**/*",
  ],
  "exclude": [
    "node_modules",
    "dist",
  ],
  "files": [
    "custom.d.ts",
  ]
}