  "license": "ISC",
  "homepage": "https://kevinjobs.github.io/horen",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinjobs/horen"
  },
  "keywords": [
    "react",
    "player",
    "typescript",
    "electron",
    "howl"
  ],
  "scripts": {
    "lint": "eslint -c .eslintrc.js --ext .ts",
