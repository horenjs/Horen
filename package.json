    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": "^5.0.0",
    "tslib": "2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "debug": "^4.3.3",
    "wait-on": "^6.0.0"
  }
