    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.10",
    "@types/request": "^2.48.8",
    "chalk": "^4.0.0",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "debug": "^4.3.3",
    "electron-rebuild": "^3.2.7",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "wait-on": "^6.0.0",
    "webpack": "^5.66.0",
